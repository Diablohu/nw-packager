{
	"name":		"nw-package-launcher",
	"main":		"____launcher____.html",
	"version":	"0.1.0",
	"chromium-args": "--mixed-context",

	"debug": 	false,

	"window": {
		"toolbar":			false,
		"frame":			false,
		"transparent":		false,

		"width": 			400,
		"height":			400,
		"icon": 			null,
		"position":			"center",
		"min_width":		100,
		"min_height":		100,
		"as_desktop":		false,
		"resizable":		false,
		"always-on-top":	false,
		"fullscreen":		false,
		"show_in_taskbar":	true,
		"show":				true,
		"kiosk":			false
	}
}