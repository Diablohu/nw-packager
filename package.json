{
  "name": "nw-packager",
  "main": "app/main.html",
  "version": "0.4.0",
  "debug": true,
  "window": {
    "width": 1280,
    "height": 720,
    "toolbar": false,
    "icon": "appicon.png",
    "position": "center",
    "min_width": 800,
    "min_height": 600,
    "as_desktop": false,
    "resizable": true,
    "always-on-top": false,
    "fullscreen": false,
    "show_in_taskbar": true,
    "frame": false,
    "show": false,
    "kiosk": false,
    "transparent": false
  },
  "repositories": [
    {
      "type": "git",
      "url": "https://github.com/Diablohu/nw-packager"
    }
  ],
  "dependencies": {
    "archiver": "1.2.0",
    "fs-extra": "^1.0.0",
    "jsonfile": "^2.4.0",
    "node-webkit-builder": "^1.0.13",
    "request": "^2.78.0",
    "rimraf": "^2.5.4",
    "adm-zip": "^0.4.7"
  },
  "devDependencies": {
    "gulp": "^3.9.0",
    "gulp-concat": "^2.6.0",
    "gulp-cssnano": "^2.1.1",
    "gulp-less": "^3.0.3",
    "gulp-minify-css": "^1.2.1",
    "gulp-notify": "^2.2.0",
    "gulp-rename": "^1.2.2",
    "gulp-uglify": "^1.4.2",
    "gulp-watch-less2": "^2.0.4",
    "less-plugin-clean-css": "^1.5.1",

    "webpack": "2.1.0-beta.28",

    "babel-core": "6.16.0",
    "babel-loader": "6.2.5",
    "babel-plugin-transform-class-properties": "6.16.0",
    "babel-plugin-transform-require-ignore": "0.0.2",
    "babel-polyfill": "6.16.0",
    "babel-preset-latest": "6.16.0",
    "babel-preset-stage-0": "6.16.0",

    "html-webpack-plugin": "2.24.1"
  },
  "babel": {
    "presets": [
      [
        "latest"
      ]
    ]
  },
  "nw-packager": {
    "platforms": [
      "win32",
      "win64",
      "osx32",
      "osx64",
      "linux32",
      "linux64"
    ],
    "filesRelative": [
      "app",
      "node_modules",
      "package.json"
    ],
    "dataVersion": {},
    "dataRelative": [],
    "macIcns": "appicon.icns",
    "winIco": "appicon.ico"
  }
}